{
  "main": "node_modules/expo/AppEntry.js",
  "author": {
    "name": "Mohammed Mulazada"
  },
  "name": "todo",
  "version": "0.0.1",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "expo": "^36.0.2",
    "react": "~16.9.0",
    "react-dom": "~16.12.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-paper": "^3.5.1",
    "react-native-reanimated": "^1.7.0",
    "react-native-screens": "^2.0.0-alpha.29",
    "react-native-web": "~0.12.2",
    "react-navigation": "^4.0.10",
    "react-navigation-tabs": "^2.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@expo/electron-adapter": "^0.0.0-alpha.12",
    "@expo/webpack-config": "^0.10.11",
    "babel-preset-expo": "~8.0.0",
    "electron": "^6.0.12",
    "electron-builder": "^22.2.0"
  },
  "private": true,
  "build": {
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ]
  }
}
